+    }
+
+    @javafx.fxml.FXML
+    public void postBudgetfordifferentDepartments(ActionEvent actionEvent) {
+        String department = DepartmentsCombox.getValue();  // Get the selected department
+        if (department == null) {
+            showAlert("No department selected", "Please select a department.");
+            return;
+        }
+
+        String budgetText = BudgetOfDepartmentsTextField.getText();  // Get the budget from the text field
+        if (budgetText.isEmpty()) {
+            showAlert("Invalid budget", "Please enter a valid budget amount.");
+            return;
+        }
+
+        double budget = 0;
+        try {
+            budget = Double.parseDouble(budgetText);  // Parse the budget as a double
+        } catch (NumberFormatException e) {
+            showAlert("Invalid input", "Please enter a valid number for the budget.");
+            return;
+        }
+
+        LocalDate date = dateofpostingDatePicker.getValue();  // Get the selected date
+        if (date == null) {
+            showAlert("No date selected", "Please select a date.");
+            return;
+        }
+
+        // Create a new DepartmentBudget object with the provided information
+        DepartmentBudget departmentBudget = new DepartmentBudget(department, budget, date);
+        departmentBudgets.put(department, departmentBudget);  // Update the map with the new budget
+
+        // Append the new budget to the binary file
+        appendBudgetToFile(departmentBudget);
+
+    }
+
+    private void showAlert(String error, String s) {
+        Alert alert = new Alert(AlertType.ERROR);
+        alert.setTitle("Error");
+        alert.setHeaderText(error);
+        alert.setContentText(s);
+        alert.showAndWait();
+
+    }
