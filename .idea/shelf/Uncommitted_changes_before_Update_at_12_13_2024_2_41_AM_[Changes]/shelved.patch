 public class UpdateMonthlygoalsController
 {
+
+    @javafx.fxml.FXML
+    private TextField BudgetOfDepartmentsTextField;
+    @javafx.fxml.FXML
+    private DatePicker dateofpostingDatePicker;
+    @javafx.fxml.FXML
+    private ComboBox<String> DepartmentsCombox;
+
+    private static String FILE_NAME = "department_budgets.bin";  // Binary file for storing budgets
+    private Map<String, DepartmentBudget> departmentBudgets = new HashMap<>();
+
