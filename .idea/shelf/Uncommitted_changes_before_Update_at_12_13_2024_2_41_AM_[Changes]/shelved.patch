+
+    private void loadAllBudgets() {
+        try (ObjectInputStream in = new ObjectInputStream(new FileInputStream(FILE_NAME))) {
+            Object readObject = in.readObject();
+
+            if (readObject instanceof Map<?, ?>) {
+                // Ensure it's a Map<String, DepartmentBudget>
+                @SuppressWarnings("unchecked")
+                Map<String, DepartmentBudget> loadedMap = (Map<String, DepartmentBudget>) readObject;
+                departmentBudgets = loadedMap;
+            } else {
+                System.out.println("The file does not contain valid budget data.");
+            }
+        } catch (IOException | ClassNotFoundException e) {
+            System.out.println("No existing budget data found or error loading data.");
+            // Ignore errors here as this is fine on the first run or if the file is empty
+        }
+    }
+
+
+    private void saveAllBudgets() {
+        try (ObjectOutputStream out = new ObjectOutputStream(new FileOutputStream(FILE_NAME))) {
+            out.writeObject(departmentBudgets);  // Serialize the map and save it to the binary file
+        } catch (IOException e) {
+            e.printStackTrace();
+            showAlert("Error saving data", "There was an error saving the department budgets.");
+        }
+    }
+    private void appendBudgetToFile(DepartmentBudget departmentBudget) {
+        // Use appendable file logic to avoid overwriting the file's previous data
+        try (FileOutputStream fileOut = new FileOutputStream(FILE_NAME, true);  // Open in append mode
+             ObjectOutputStream out = new ObjectOutputStream(fileOut)) {
+
+            // Prevent writing the header again if file already has data
+            if (new File(FILE_NAME).length() > 0) {
+                out.reset();  // Reset to avoid writing a new header
+            }
+
+            out.writeObject(departmentBudget);  // Serialize and append the department budget object
+
+        } catch (IOException e) {
+            e.printStackTrace();
+            showAlert("Error saving data", "There was an error saving the department budget.");
+        }
+    }
+
+
+
+
